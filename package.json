{
  "name": "entersys-frontend",
  "version": "2025.10.12-0901-983b48f",
  "description": "Frontend web application for Entersys.mx project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode production",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --mode production",
    "preview:staging": "vite preview --mode staging",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "deploy:local": "node scripts/deploy.js local",
    "deploy:server": "node scripts/deploy.js server",
    "deploy:help": "node scripts/deploy.js help",
    "version:bump": "node scripts/deploy.js version",
    "pwa:icons": "node scripts/generate-pwa-icons.js",
    "pwa:screenshots": "node scripts/generate-pwa-screenshots.js",
    "pwa:all": "npm run pwa:icons && npm run pwa:screenshots",
    "security:headers": "node scripts/generate-security-headers.js",
    "clean": "rm -rf dist node_modules/.vite src/version.json src/deploy-info.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armando-entersys/entersys-frontend.git"
  },
  "keywords": [
    "entersys",
    "frontend",
    "web",
    "react",
    "vite"
  ],
  "author": "Entersys Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/armando-entersys/entersys-frontend/issues"
  },
  "homepage": "https://github.com/armando-entersys/entersys-frontend#readme",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@relume_io/relume-tailwind": "^1.3.0",
    "@relume_io/relume-ui": "^1.2.0",
    "framer-motion": "^10.16.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.3.3",
    "terser": "^5.36.0",
    "vite": "^4.4.5"
  }
}